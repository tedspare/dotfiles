export BUN_INSTALL="$HOME/.bun"
export PATH="$BUN_INSTALL/bin:$PATH"
export PATH="$HOME/.local/bin:$PATH"

# Git
alias gat='git status'
alias gcm='git commit -am'
alias gas='git stash'
alias gul='git pull'

# Zsh
alias soz='source ~/.zshrc'
alias coz='code ~/.zshrc'

# Bun
alias bi='bun install'

# nav
alias cdo='cd ~'
